{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 0,
        "VariableDeclarator": 1,
        "outerIIFEBody": 1,
        "FunctionDeclaration": {
          "parameters": 1,
          "body": 1
        },
        "FunctionExpression": {
          "parameters": 1,
          "body": 1
        }
      }
    ]
  }
}
